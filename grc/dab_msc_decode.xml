<?xml version="1.0"?>
<block>
  <name>MSC Decoder</name>
  <key>dab_msc_decode</key>
  <category>dab</category>
  <import>import dab</import>
  <make>dab.msc_decode($dab_params, $address, $size, $protection, $verbose, $debug)</make>
  <param>
    <name>dab_params</name>
    <key>dab_params</key>
    <type>raw</type>
  </param>
  <param>
    <name>address</name>
    <key>address</key>
    <type>int</type>
  </param>
  <param>
    <name>size</name>
    <key>size</key>
    <type>int</type>
  </param>
  <param>
    <name>protection</name>
    <key>protection</key>
    <type>int</type>
  </param>
  <param>
    <name>verbose</name>
    <key>verbose</key>
    <type>bool</type>
  </param>
  <param>
    <name>debug</name>
    <key>debug</key>
    <type>bool</type>
  </param>
  <sink>
    <name>data</name>
    <type>float</type>
  </sink>
  <sink>
    <name>trigger</name>
    <type>byte</type>
  </sink>
  <source>
    <name>out</name>
    <type>byte</type>
  </source>
</block>
