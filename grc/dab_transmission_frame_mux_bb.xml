<block>
  <name>Transmission frame MUX</name>
  <key>dab_transmission_frame_mux_bb</key>
  <category>DAB</category>
  <import>import dab</import>
  <make>dab.transmission_frame_mux_bb($transmission_mode, $subch_size)</make>
  <param>
    <name>Transmission Mode</name>
    <key>transmission_mode</key>
    <type>int</type>
  </param>
  <param>
    <name>Sub-channel size</name>
    <key>subch_size</key>
    <type>raw</type>
  </param>
  <sink>
    <name>FIC</name>
    <type>byte</type>
    <nports>1</nports>
  </sink>
  <sink>
    <name>sub-channel</name>
    <type>byte</type>
    <nports>7</nports>
  </sink>
  <source>
    <name>data</name>
    <type>byte</type>
    <nports>1</nports>
  </source>
  <source>
    <name>trigger</name>
    <type>byte</type>
    <nports>1</nports>
  </source>
</block>
