<?xml version="1.0"?>
<block>
  <name>OFDM sync v2</name>
  <key>dab_ofdm_sync_dab2</key>
  <category>[DAB]</category>
  <import>import dab</import>
  <make>dab.ofdm_sync_dab2(dab.parameters.dab_parameters(mode=$mode, sample_rate=$samp_rate, verbose=False), dab.parameters.receiver_parameters(mode=$mode), False)</make>
  <param>
    <name>Mode</name>
    <key>mode</key>
    <type>int</type>
  </param>
  <param>
    <name>Samp_rate</name>
    <key>samp_rate</key>
    <type>int</type>
  </param>
  <sink>
    <name>in</name>
    <type>complex</type>
  </sink>
  <source>
    <name>out</name>
    <type>complex</type>
  </source>
  <source>
    <name>trigger</name>
    <type>byte</type>
  </source>
</block>
